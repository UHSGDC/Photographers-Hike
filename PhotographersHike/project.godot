[application]

config/name="PhotographersHike"
config/description="We are the University High School Game Development Club, also known as UHSGDC. As a team, we are passionate about creating video games and are always looking for new challenges to push our skills to the limit. For the 2022-2023 school year, we have decided to take on an ambitious project called \\\"Photographer's Hike.\\\"  We have a dedicated faculty advisor to guide us, and we are confident that with our collective creativity and skills, we will be able to bring this game to life. We can't wait to see all of our hard work pay off and share our creation with the world and soo here we have it!"
run/main_scene="res://Game.tscn"
boot_splash/bg_color=Color( 0, 0, 0, 1 )
config/icon="res://icon.png"

[autoload]

Global="*res://Global/Global.tscn"
InputHelper="*res://addons/input_helper/input_helper.gd"
JsonOpener="*res://Dialog/JSONOpener.gd"

[display]

window/size/width=480
window/size/height=288
window/size/test_width=1440
window/size/test_height=864
window/stretch/mode="2d"
window/stretch/aspect="keep"

[editor_plugins]

enabled=PoolStringArray( "res://addons/input_helper/plugin.cfg" )

[gui]

common/drop_mouse_on_gui_input_disabled=true

